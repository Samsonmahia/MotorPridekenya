{
  "name": "motorpridekenya",
  "version": "1.0.0",
  "description": "MotorPride Kenya - Premium Vehicle Marketplace",
  "type": "commonjs",
  "scripts": {
    "generate-index": "node generateCarsIndex.js",
    "build": "npm run generate-index && npm run site-build",
    "site-build": "npm run build-local",
    "build-local": "echo '⚙️ Building static site...' && npx live-server --port=3000",
    "dev": "npm run generate-index && npx live-server --port=3000",
    "test": "node scripts/test-car-files.js",
    "setup": "npm run generate-index && node scripts/create-image-structure.js && echo '✅ Setup complete!'",
    "emergency-build": "node scripts/emergency-build.js"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "live-server": "^1.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "cars",
    "kenya",
    "marketplace",
    "vehicle",
    "automotive"
  ],
  "author": "MotorPride Kenya",
  "license": "MIT"
}
